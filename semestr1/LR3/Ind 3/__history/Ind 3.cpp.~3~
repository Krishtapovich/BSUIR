#pragma hdrstop
#pragma argsused

#include <iostream>
#include <tchar.h>
#include <math.h>
#include <stdio.h>
#include <conio.h>
using namespace std;

int _tmain() {
	float x, y;
	cout << "y" << '\t' << "x" << endl;
	for (float i = 3; i <= 4; i += 0.2) {
		y = sqrt(i) * cosh(i);
		cout << y << '\t' << i << endl;
	}
	getch();
	return 0;
}
