#pragma hdrstop
#pragma argsused

#include <iostream>
#include <conio.h>

#include <stdio.h>
using namespace std;

int _tmain() {
	int arr[3][3] = { {10, 2, 3}, {4, 5, 0}, {1, 5, 9}};
	int min = arr[2][2];
	for (int i = 0; i < 3; i++) {
		for (int j = 0; j < 3; j++) {
			if (i > j) {
				if (arr[i][j] < min) {
					min = arr[i][j];
				}
			}
		}
	}
	cout << "The minimum element of the array is - " << min;
	getch();
	return 0;
}
