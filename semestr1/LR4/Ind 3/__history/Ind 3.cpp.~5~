#pragma hdrstop
#pragma argsused

#include <iostream>
#include <stdio.h>
#include <conio.h>
#include <ctime>
using namespace std;

int _tmain() {
	srand(time(NULL));
	int size = 0, summin = 0, summax = 0;
	cin >> size;
	int **arr = new int*[size];
	for (int i = 0; i < size; i++) {
		arr[i] = new int[size];
	}
	for (int i = 0; i < size; i++) {
		for (int j = 0; j < size; j++) {
			arr[i][j] = rand() % 10 + 1;
		}
	}
	for (int i = 0; i < size; i++) {
		for (int j = 0; j < size; j++) {
			cout << arr[i][j] << '\t';
		}
		cout << endl << endl;
	}
	delete[]arr;
	getch();
	return 0;
}
